{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "rangeStrategy": "bump"
    }
  ],
  "schedule": ["every day"],
  "timezone": "Europe/Vienna",
  "labels": ["dependencies"],
  "assignees": ["sjerocom"],
  "vulnerabilityAlerts": {
    "enabled": true,
    "labels": ["security"]
  }
}