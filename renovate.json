{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "repositories": [
    "sjerocom/pv-monitor-card"
  ],
  "packageRules": [
    {
      "rangeStrategy": "bump",
      "matchPackageNames": [
        "*"
      ]
    }
  ],
  "timezone": "Europe/Vienna",
  "labels": [
    "dependencies"
  ],
  "assignees": [
    "sjerocom"
  ],
  "vulnerabilityAlerts": {
    "enabled": true,
    "labels": [
      "security"
    ]
  },
  "dependencyDashboard": true,
  "prConcurrentLimit": 5,
  "prHourlyLimit": 0
}
